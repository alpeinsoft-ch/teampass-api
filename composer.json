{
    "name": "alpeinsoft-ch/teampass-api",
    "description": "API for teampass",
    "keywords": [ "api", "teampass" ],
    "homepage": "https://github.com/alpeinsoft-ch/teampass-api",
    "license": "MIT",
    "authors": [
        {
            "name": "ALPEIN Software SWISS Team",
            "homepage": "http://www.alpeinsoft.ch/",
            "email": "contact@alpeinsoft.ch"
        }
    ],
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "gilfether/phpcrypt",
                "version": "0.5.1",
                "source": {
                    "url": "git@github.com:gilfether/phpcrypt.git",
                    "type": "git",
                    "reference":"v0.5.1"
                },
                "autoload": {
                    "classmap": ["phpCrypt.php"]
                }
            }
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "doctrine/dbal": "~2.5",
        "gilfether/phpcrypt": "^0.5",
        "incenteev/composer-parameter-handler": "~2.1",
        "passwordlib/passwordlib": "@dev",
        "phpseclib/phpseclib": "~2.0",
        "roderik/pwgen-php": "@stable",
        "silex/silex": "~1.3",
        "symfony/finder": "~2.7",
        "symfony/translation": "~2.7"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "^1.10"
    },
    "autoload": {
        "psr-4": {
            "Teampass\\Api\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "platform": {
            "php": "5.5.9"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        },
        "incenteev-parameters": {
            "file": "app/config.yml"
        }
    }
}
